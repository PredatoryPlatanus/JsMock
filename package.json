    "test": "node_modules/.bin/karma start test/karma.conf.js --browsers Firefox --single-run --no-auto-watch",
    "testwatch": "node_modules/bin/karma start test/karma.conf.js  --autowatch",
    "shelljs": "node -e \"require('shelljs/global');\""
